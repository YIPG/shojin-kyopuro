    "n, k = list(map(int, input().split()))\n",
    "ans = 0\n",
    "for i in range(1,n+1):\n",
    "    tmp=1\n",
    "    while i * tmp < k:\n",
    "        tmp=tmp*2\n",
    "    ans+=1/tmp\n",
