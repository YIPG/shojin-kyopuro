     "text": [
      "0-2+9+0=7\n"
     ]
    }
   ],
   "source": [
    "from itertools import product\n",
    "s = input()\n",
    "options = product([\"+\", \"-\"], repeat=3)\n",
    "for op in options:\n",
    "    v = s[0]\n",
    "    for i in range(3):\n",
    "        v += op[i] + s[i+1]\n",
    "    if eval(v) == 7:\n",
    "        break\n",
    "print(v+\"=7\")"
   ]
