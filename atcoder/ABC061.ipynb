   "source": [
    "from itertools import product\n",
    "s = input()\n",
    "\n",
    "n = len(s) - 1\n",
    "ans = 0\n",
    "\n",
    "plus = list(product([\"\",\"+\"], repeat=n))\n",
    "\n",
    "for p in plus:\n",
    "    e = s[0]\n",
    "    for i in range(n):\n",
    "        e += p[i]+s[i+1]\n",
    "    print(e)\n",
    "    ans += eval(e)\n",
    "print(ans)"
   ]
